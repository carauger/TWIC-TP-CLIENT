<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>Accueil</servlet-name>
		<servlet-class>servlet.Accueil</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Accueil</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ModifierVille</servlet-name>
		<servlet-class>servlet.ModifierVille</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ModifierVille</servlet-name>
		<url-pattern>/modifVille</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AjoutVille</servlet-name>
		<servlet-class>servlet.AjoutVille</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AjoutVille</servlet-name>
		<url-pattern>/ajoutVille</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SupprimerVille</servlet-name>
		<servlet-class>servlet.SupprimerVille</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SupprimerVille</servlet-name>
		<url-pattern>/supprVille</url-pattern>
	</servlet-mapping>
</web-app>